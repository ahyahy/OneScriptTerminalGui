	|                return 0;
	|            while (max >= min)
	|            {
	|                mid = (min + max) / 2;
	|                if (rune > table[mid, 1])
	|                    min = mid + 1;
	|                else if (rune < table[mid, 0])
	|                    max = mid - 1;
	|                else
	|                    return 1;
	|            }
	|
	|            return 0;
	|        }
