	|                    {
	|                        return i;
	|                    }
	|                }
	|                else if (t is string s)
	|                {
	|                    if (s.StartsWith(search, StringComparison.InvariantCultureIgnoreCase))
	|                    {
	|                        return i;
	|                    }
	|                }
	|            }
	|            return -1;
	|        }
	|    }
