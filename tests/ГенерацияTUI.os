	|                    LayoutStyle = LayoutStyle.Computed,
	|                    TextAlignment = TextAlignment.Centered,
	|                    X = Pos.Center(),
	|                    Y = Pos.Center(),
	|                    Width = Dim.Fill(),
	|                    Height = Dim.Fill(1),
