	|                        return contents = ustring.Make(Application.Driver.Clipboard.GetClipboardData());
	|                    }
	|                    else
	|                    {
	|                        return contents;
	|                    }
	|                }
	|                catch (Exception)
	|                {
	|                    return contents;
	|                }
	|            }
	|            set
	|            {
	|                try
	|                {
	|                    if (IsSupported)
	|                    {
	|                        if (value == null)
	|                        {
	|                            value = string.Empty;
	|                        }
	|                        Application.Driver.Clipboard.SetClipboardData(value.ToString());
