	|        void TextField_TextChanged(ustring e)
	|        {
	|            try
	|            {
	|                if (!TimeSpan.TryParseExact(Text.ToString().Trim(), format.Trim(), CultureInfo.CurrentCulture, TimeSpanStyles.None, out TimeSpan result))
	|                    Text = e;
