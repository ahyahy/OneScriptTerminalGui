	|            if (maxWidthLine > Application.Driver.Cols)
	|            {
	|                maxWidthLine = Application.Driver.Cols;
	|            }
	|            if (width == 0)
	|            {
	|                maxWidthLine = Math.Max(maxWidthLine, defaultWidth);
	|            }
	|            else
	|            {
	|                maxWidthLine = width;
