	|            if (historyTextItems?.Count > 0 && idxHistoryText > 0)
	|            {
	|                IsFromHistory = true;
	|
	|                idxHistoryText--;
	|
	|                var historyTextItem = new HistoryTextItem(historyTextItems[idxHistoryText])
	|                {
