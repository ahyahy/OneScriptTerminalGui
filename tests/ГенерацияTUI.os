	|        int GetFormatIndex(ustring[] fm, string t)
	|        {
	|            int idx = -1;
	|            for (int i = 0; i < fm.Length; i++)
	|            {
	|                if (fm[i].Contains(t))
	|                {
	|                    idx = i;
	|                    break;
	|                }
	|            }
	|            return idx;
	|        }
