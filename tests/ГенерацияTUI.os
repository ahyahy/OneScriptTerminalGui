	|
	|        public void Redo()
	|        {
	|            if (historyTextItems?.Count > 0 && idxHistoryText < historyTextItems.Count - 1)
	|            {
	|                IsFromHistory = true;
	|
	|                idxHistoryText++;
	|
	|                var historyTextItem = new HistoryTextItem(historyTextItems[idxHistoryText])
	|                {
	|                    IsUndoing = false
	|                };
