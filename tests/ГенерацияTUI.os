	|                {
	|                    Height = msgboxHeight
	|                };
	|            }
	|            else
	|            {
	|                d = new Dialog(title, width, Math.Max(height, 4), buttonList.ToArray());
	|            }
	|
	|            if (border != null)
	|            {
	|                d.Border = border;
	|            }
	|
	|            if (useErrorColors)
	|            {
	|                d.ColorScheme = Colors.Error;
	|            }
	|
	|            if (message != null)
	|            {
	|                var l = new Label(message)
