				Стр506 = СокрЛП(Стр506);
				// Сообщить("Стр506 = " + Стр506);
			КонецЕсли;
			М507 = СтрРазделить(Стр506, " ");
			КлассРус = М507[0];
			СобытиеРус = М507[1];
			КлассАнгл = М507[2];
