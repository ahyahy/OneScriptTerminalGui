	|                selection = ((long)(uint)selectionStartRow << 32) | (uint)selectionStartColumn;
	|                point = ((long)(uint)currentRow << 32) | (uint)currentColumn;
	|            }
	|            else
	|            {
	|                selection = ((long)(uint)startRow << 32) | (uint)startCol;
	|                point = ((long)(uint)cRow << 32) | (uint)cCol;
	|            }
	|            if (selection > point)
	|            {
	|                start = point;
	|                end = selection;
	|            }
	|            else
	|            {
	|                start = selection;
	|                end = point;
	|            }
	|        }
