	|        public void AddTab(Tab tab, bool andSelect)
	|        {
	|            if (tabs.Contains(tab))
	|            {
	|                return;
	|            }
	|
	|
	|            tabs.Add(tab);
	|
	|            if (SelectedTab == null || andSelect)
