	|                d.Width = Math.Min(Math.Max(maxWidthLine, Math.Max(title.ConsoleWidth, Math.Max(textWidth + 2, d.GetButtonsWidth() + d.buttons.Count + 2))), Application.Driver.Cols); // textWidth + (left + padding + padding + right)
	|            }
	|
	|            // Setup actions
