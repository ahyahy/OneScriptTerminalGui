	|        public static (int exitCode, string result) Bash(string commandLine, string inputText = """", bool waitForOutput = false)
	|        {
	|            var arguments = $""-c \""{commandLine}\"""";
	|            var (exitCode, result) = Process(""bash"", arguments, inputText, waitForOutput);
